package com.FlipKart.testCases;

import org.testng.Assert;
import org.testng.annotations.Test;

import com.FlipKart.PageObjects.LoginFlipkart;

public class TC1_LoginTest extends BaseClass{
	
	@Test
	public void loginTest()
	{
		LoginFlipkart loginFlip =new LoginFlipkart(driver);
		loginFlip.setMobileNo(mblNo);
		loginFlip.setPassword(password);
		loginFlip.clickLogin();

		String actual =driver.getTitle();
		String expected ="Online Shopping Site for Mobiles, Electronics, Furniture, Grocery, Lifestyle, Books &amp; More. Best Offers!";

		if(actual.equals(expected))
		{
			Assert.assertTrue(true);
		}
		else
		{
			Assert.assertFalse(false);
		}	
	}
}